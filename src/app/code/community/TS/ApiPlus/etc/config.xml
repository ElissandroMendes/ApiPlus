<config>
    <modules>
        <TS_ApiPlus>
            <version>0.1.0.0</version>
        </TS_ApiPlus>
    </modules>
    <global>
        <models>
            <ts_apiplus>
                <class>TS_ApiPlus_Model</class>
            </ts_apiplus>
        </models>
        <blocks>
            <ts_apiplus>
                <class>TS_ApiPlus_Block</class>
            </ts_apiplus>
        </blocks>
        <helpers>
            <ts_apiplus>
                <class>TS_ApiPlus_Helper</class>
            </ts_apiplus>
        </helpers>
    </global>
    <frontend>
        <translate>
            <modules>
                <ts_apiplus>
                    <files>
                        <default>TS/ApiPlus/Frontend.csv</default>
                    </files>
                </ts_apiplus>
            </modules>
        </translate>
        <routers>
            <api>
                <args>
                    <modules>
                        <ts_apiplus before="Mage_Api">TS_ApiPlus</ts_apiplus>
                    </modules>
                </args>
            </api>
        </routers>
    </frontend>
    <adminhtml>
        <translate>
            <modules>
                <ts_apiplus>
                    <files>
                        <default>TS/ApiPlus/Adminhtml.csv</default>
                    </files>
                </ts_apiplus>
            </modules>
        </translate>
    </adminhtml>
</config>
