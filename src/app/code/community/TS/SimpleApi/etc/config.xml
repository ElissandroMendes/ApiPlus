<config>
    <modules>
        <TS_SimpleApi>
            <version>0.1.0.0</version>
        </TS_SimpleApi>
    </modules>
    <global>
        <models>
            <ts_simpleapi>
                <class>TS_SimpleApi_Model</class>
            </ts_simpleapi>
        </models>
        <blocks>
            <ts_simpleapi>
                <class>TS_SimpleApi_Block</class>
            </ts_simpleapi>
        </blocks>
        <helpers>
            <ts_simpleapi>
                <class>TS_SimpleApi_Helper</class>
            </ts_simpleapi>
        </helpers>
    </global>
    <frontend>
        <translate>
            <modules>
                <ts_simpleapi>
                    <files>
                        <default>TS/SimpleApi/Frontend.csv</default>
                    </files>
                </ts_simpleapi>
            </modules>
        </translate>
    </frontend>
    <adminhtml>
        <translate>
            <modules>
                <ts_simpleapi>
                    <files>
                        <default>TS/SimpleApi/Adminhtml.csv</default>
                    </files>
                </ts_simpleapi>
            </modules>
        </translate>
    </adminhtml>
</config>
